package test.demo;

public class BubleSort implements SortKhanh {
	private int[] arr;
	@Override
	public int[] sort(int[] arr) {
		int n = arr.length;
        for (int i = 0; i < n - 1; i++)
            for (int j = 0; j < n - i - 1; j++)
                if (arr[j] > arr[j + 1]) {
                    int temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
        }
        
		return this.arr;
	}

	@Override
	public void print() {
		// TODO Auto-generated method stub
		
	}
}
